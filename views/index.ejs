  <div
    class="alert alert-<%=feedback.type.toLowerCase() == 'error' ? 'danger' : feedback.type.toLowerCase() %> alert-dismissible fade show"
    role="alert" style="position: absolute; top: 7vh; left: 50%; transform: translateX(-50%);">
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    <strong>
      <%=feedback.type='' %>
    </strong>
    <%=feedback.message %>
  </div>
  <% } %>
<script src="/vendors/bootstrap/js/jquery.min.js"></script>
    <script>
      $('.alert-dismissible').delay(5000).fadeOut()
    </script>
    <script>
        <% if (locals.formErrors) { %>
    <% for (const name in locals.formBody) {
            const value = locals.formBody[name]; %>
            var input = document.querySelector('[name=<%-name%>]')
            if (input.type == "radio") {
              $('[name=<%- name %>][value=<%- value %>]').prop('checked', true)
            } else {
              input.value = "<%-value %>"
            }
    <% } %>
    <% for (const name in locals.formErrors) {
            const msg = locals.formErrors[name].msg; %>
              document.querySelector('[name=<%- name %>]')?.classList.add('is-invalid')
            $('[name=<%- name %>]').parent().append('<div class="alert mt-4 alert-danger"><%- msg %></div>')
              <% } %>
  <% } %>
    </script>
